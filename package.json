{
  "name": "angular2-full-stack",
  "version": "12.0.3",
  "license": "MIT",
  "author": "Davide Violante",
  "description": "Angular Full Stack project built using Angular 2+, Express, Mongoose and Node.",
  "engines": {
    "node": "14.17.0",
    "npm": "7.14.0"
  },
  "scripts": {
    "ng": "ng",
    "builddev": "ng build -c development && tsc -p server",
    "build": "ng build && tsc -p server",
    "start": "node dist/server/app.js",
    "predev": "tsc -p server",
    "dev": "concurrently \"mongod\" \"ng serve --open\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build && tsc -p server && node dist/server/app.js\"",
    "test": "ng test",
    "testbe": "tsc -p server && mocha dist/server/test --exit",
    "lint": "ng lint && htmlhint \"client/**/*.html\" && sass-lint \"client/**/*.scss\" -v && tslint server/**/**.ts{,x}",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.5",
    "@angular/common": "^11.0.5",
    "@angular/compiler": "^11.0.5",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^11.0.5",
    "@angular/platform-browser": "^11.0.5",
    "@angular/platform-browser-dynamic": "^11.0.5",
    "@angular/router": "^11.0.5",
    "@auth0/angular-jwt": "^5.0.2",
    "@ng-bootstrap/ng-bootstrap": "6.0.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "4.6.0",
    "chart.js": "2.9.4",
    "core-js": "3.8.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.13",
    "morgan": "^1.10.0",
    "ngx-toastr": "13.2.0",
    "popper.js": "^1.16.1",
    "rxjs": "6.6.3",
    "rxjs-compat": "^6.5.5",
    "sequelize": "^6.6.2",
    "tether": "^1.4.7",
    "tslib": "2.0.3",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^12.0.4",
    "@angular-devkit/build-angular": "^12.0.3",
    "@angular/cli": "^11.0.5",
    "@angular/language-service": "^11.0.5",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.12",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^14.17.3",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "codelyzer": "^6.0.2",
    "concurrently": "^6.2.0",
    "htmlhint": "^0.14.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.6.0",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.7",
    "protractor": "~7.0.0",
    "sass-lint": "^1.13.1",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.3",
    "typescript": "4.0.3"
  }
}
